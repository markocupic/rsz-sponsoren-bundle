{
    "name": "markocupic/rsz-sponsoren-bundle",
    "description": "RSZ Sponsoren Frontend- und Backend Modul",
    "keywords": [
        "contao",
        "bundle",
        "RSZ",
        "Regionalkader Sportklettern Znetralschweiz",
        "Sponsoren"
    ],
    "type": "contao-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Marko Cupic",
            "email": "m.cupic@gmx.ch",
            "homepage": "https://github.com/markocupic",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/markocupic/rsz-sponsoren-bundle/issues",
        "source": "https://github.com/markocupic/rsz-sponsoren-bundle"
    },
    "require": {
        "php": "^8.1",
        "contao/core-bundle": "^4.13 || ^5.0"
    },
    "require-dev": {
        "contao/test-case": "^4.0",
        "contao/php-cs-fixer": "^2.2",
        "contao/manager-plugin": "^2.3",
        "phpunit/phpunit": "^8.4",
        "symfony/http-client": "4.4.*",
        "symfony/phpunit-bridge": "4.4.*"
    },
    "autoload": {
        "psr-4": {
            "Markocupic\\RszSponsorenBundle\\": "src/"
        }
    },
    "extra": {
        "contao-manager-plugin": "Markocupic\\RszSponsorenBundle\\ContaoManager\\Plugin"
    }
}
